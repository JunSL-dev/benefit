if !sess.login
    script.
        location.replace('/')

doctype html
html(lang='en', dir='ltr')
    head
        meta(charset='utf-8', name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no')
        |     
        title Benefit
        |     
        link(rel='stylesheet', href='https://use.fontawesome.com/releases/v5.5.0/css/all.css', integrity='sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU', crossorigin='anonymous')
        |     
        script(rel='javascript', src='https://code.jquery.com/jquery-3.3.1.min.js')
        // jQuery
        script(rel='javascript', src='js/main.js')
        |     
        link(rel='stylesheet', href='css/style.css')
    |   
    body
        .navbar
            img.logo(src='img/logo.png')
            |       
            .btn-box
                a.btn About Us
                |         
                a.btn My Page
                |         
                a.btn Contact Us
            |       
            .login_state
                a(href='/login')
                    img(src='img/profile-img.png')
                |         
                span.name
                    span.name_data(style="font-family:'Kopub Bold'") #{sess.name}
                    |  님
                |  | 
                a.signout(href='/logout') 로그아웃
            |       
            img.background(src='img/nav-background.png')
        |     
        .main-box
            .header
            |       
            p.text
                span(style='font-weight: bold')
                    | 베네
                    span(style='color: #354593') 핏
                | 은 여러분이 받을 수 있는 복지 혜택을
                br
                span(style='font-weight: bold') 개인 맞춤형
                | 으로 알려드립니다.
            |       
            .bottom-box
                h1.title
                    span(style='font-weight: bold') #{sess.name}
                    |  님 현재 받을 수 있는 복지 혜택
                |         
                a.scroll.scroll-btn(href='#result')
                    i.fas.fa-arrow-down
        |     
        #result.result
            if cnt.life != 0
                .item
                    .title-box
                        img.icon(src='img/home.png')
                        |           
                        span.title
                            span(style="color: #354593; font-family: 'Kopub Bold'") 생활
                            |  지원
                        // 여성가족부
                    |         
                    ul
                        each val in data
                            if val.jurMnofNm == "여성가족부" || val.jurMnofNm == "고용노동부" || val.jurMnofNm == "국토교통부" || val.jurMnofNm == "국가보훈처"
                                li
                                    a(href='#{val.servDtlLink}' target='_blank')= val.servNm
                |       
            if cnt.edu != 0
                .item
                    .title-box
                        img.icon(src='img/school.png')
                        |           
                        span.title
                            span(style="color: #354593; font-family: 'Kopub Bold'") 교육
                            |  지원
                        // 교육부
                    |         
                    ul
                        each val in data
                            if val.jurMnofNm == "교육부" || val.jurMnofNm == "문화체육관광부"
                                li
                                    a(href='#{val.servDtlLink}' target='_blank')= val.servNm
                |      
            if cnt.eco != 0
                .item
                    .title-box
                        img.icon(src='img/coin.png')
                        |           
                        span.title
                            span(style="color: #354593; font-family: 'Kopub Bold'") 경제
                            |  지원
                        // 기획 재정부
                    |         
                    ul
                        each val in data
                            if val.jurMnofNm == "기획재정부" || val.jurMnofNm == "산업통상자원부"
                                li
                                    a(href='#{val.servDtlLink}' target='_blank')= val.servNm
                |      
            if cnt.medic != 0
                .item
                    .title-box
                        img.icon(src='img/medical.png')
                        |           
                        span.title
                            span(style="color: #354593; font-family: 'Kopub Bold'") 의료
                            |  지원
                        // 보건복지부
                    |         
                    ul
                        each val in data
                            if val.jurMnofNm == "보건복지부"
                                li
                                    a(href='#{val.servDtlLink}' target='_blank')= val.servNm
                |    
            if cnt.tech != 0
                .item
                    .title-box
                        img.icon(src='img/wifi.png')
                        |           
                        span.title
                            span(style="color: #354593; font-family: 'Kopub Bold'") 기술
                            |  지원
                        // 과학기술정보통신부
                    |         
                    ul
                        each val in data
                            if val.jurMnofNm == "과학기술정보통신부"
                                li
                                    a(href='#{val.servDtlLink}' target='_blank')= val.servNm
                |   
            if cnt.culture != 0 
                .item
                    .title-box
                        img.icon(src='img/turnup.png')
                        |           
                        span.title
                            span(style="color: #354593; font-family: 'Kopub Bold'") 문화
                            |  지원
                        // 문화체육관광부
                    |         
                    ul
                        each val in data
                            if val.jurMnofNm == "문화체육관광부"
                                li
                                    a(href='#{val.servDtlLink}' target='_blank')= val.servNm
                |       
            if cnt.area != 0
                .item
                    .title-box
                        img.icon(src='img/local.png')
                        |           
                        span.title
                            span(style="color: #354593; font-family: 'Kopub Bold'") 지역
                            |  지원
                        // 농림축산식품부
                    |         
                    ul
                        each val in data
                            if val.jurMnofNm == "농림축산식품부" || val.jurMnofNm == "환경부"
                                li
                                    a(href='#{val.servDtlLink}' target='_blank')= val.servNm
